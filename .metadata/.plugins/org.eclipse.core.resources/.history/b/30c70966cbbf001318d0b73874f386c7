import java.awt.*;
import javax.swing.JPanel;


public class Schema extends JPanel{
	private JPanel[] pl = new JPanel[5];
	private Lektion[] platser= new Lektion[25];
	private Elev e;
	private Lärare l;
	
	public Schema(){
		this.setPreferredSize(new Dimension(1000,600));
		this.setBackground(Color.white); 
		this.setLayout(new GridLayout(1,5));
		pl[0].setPreferredSize(new Dimension(200,600));
		this.add(pl[0]);
		pl[1].setPreferredSize(new Dimension(200,600));
		this.add(pl[1]);
		pl[2].setPreferredSize(new Dimension(200,600));
		this.add(pl[2]);
		pl[3].setPreferredSize(new Dimension(200,600));
		this.add(pl[3]);
		pl[4].setPreferredSize(new Dimension(200,600));
		this.add(pl[4]);
		setVisible(true);
	}
}

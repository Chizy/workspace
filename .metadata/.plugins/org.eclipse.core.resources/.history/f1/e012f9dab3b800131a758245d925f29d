import javax.swing.JOptionPane;


public class Bil {
	private String reg;
	private String märke;
	private Bilägare ägare;
	
	public Bil(){
		reg = JOptionPane.showInputDialog("Registrerings nr.?");
		märke = JOptionPane.showInputDialog("Märke?");
	}

	public void säljd(){
		ägare = null;
	}
	
	public void köpt(Bilägare b){
		ägare = b;
	}



}

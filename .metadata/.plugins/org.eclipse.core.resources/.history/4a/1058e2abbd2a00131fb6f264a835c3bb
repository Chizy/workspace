public class Testkör {

	public static void main(String [] arg){
		int i=0;
		int u=0;
		Kort k1[] =new Kort[52];
		int valörenF=(int)(13*Math.random()+1);
		int färgenF=(int)(4*Math.random()+1);
		k1[u] = new Kort(valörenF, färgenF);
		Kort kk=new Kort(valörenF, färgenF);
		//String s1 = k1[u].toString();
		String s2 = kk.toString();
		String[] k = kk;
		String[] k2 = k;
		k2[u] = k1[u].toString();
		
		while(u<52){
			
			for(i=0;i<k1.length;i++){
				
				if(k2[u].equals(s2)){
				
					int valören=(int)(13*Math.random()+1);
					int färgen=(int)(4*Math.random()+1);
					kk = new Kort(valören, färgen);
					s2 = kk.toString();
					k2[u] = k1[u].toString();
					i=0;
				
				}else{
					
					k1[u]=kk;
					System.out.println((u+1)+" "+k1[u].toString());
					u++;
					int valören=(int)(13*Math.random()+1);
					int färgen=(int)(4*Math.random()+1);
					kk = new Kort(valören, färgen);
					s2 = kk.toString();
					i=500;
				}
			}
		}
	}
}
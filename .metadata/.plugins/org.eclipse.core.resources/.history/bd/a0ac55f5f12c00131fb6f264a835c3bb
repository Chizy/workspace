import javax.swing.JOptionPane;


public class MirrorWord {
	public static void main(String[] args) {
		int i=0;
		String s1= JOptionPane.showInputDialog("Ord?");
		
		while(i<=(s1.length()*2)-1){
			if(i<=s1.length()-1){
				System.out.print(s1.charAt(i));
		}
			else if(i<=(s1.length()*2)){
				System.out.print(s1.charAt(i));
		}
		i++;
		}
	}

}

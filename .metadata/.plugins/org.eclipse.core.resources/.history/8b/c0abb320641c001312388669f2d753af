import javax.swing.*;

public class Ränt {
	public static void main(String[] args) {
		String blash = null;
		String indata;
		double summa=1000;
		double r=1.05;
		indata = JOptionPane.showInputDialog("År?");
		int å = Integer.parseInt(indata);
		for (int i=0; i<å; i++){
			blash = blash + "År "+ i + ":" + summa;
			summa= summa * r;
			
		}
		JOptionPane.showMessageDialog(null, blash + "\n");

	}

}

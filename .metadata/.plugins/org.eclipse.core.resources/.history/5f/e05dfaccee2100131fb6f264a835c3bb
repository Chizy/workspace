import javax.swing.JOptionPane;


public class Testkör {

	
	public static void main(String [] arg){
		
		Hisstest h1=new Hisstest();
		String indata= JOptionPane.showInputDialog("Våning?");
		int v=Integer.parseInt(indata);
		h1.körTill(v);
		
		JOptionPane.showMessageDialog(null,"Du är på våning: "+h1.vilkenVåning());
		
	}
	
	
	
}
